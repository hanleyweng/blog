      package {          import flash.display.Sprite;          import flash.events.Event;          public class main extends Sprite {              public function main() {                  var my_sprite = new Sprite();                  addChild(my_sprite);                  my_input = new keys(my_sprite);                  stage.addEventListener(Event.ENTER_FRAME,on_enter_frame);              }              public function on_enter_frame(event:Event) {                  if (my_input.is_left()) {                      trace("left");                  }                  if (my_input.is_right()) {                      trace("right");                  }                  if (my_input.is_up()) {                      trace("up");                  }                  if (my_input.is_down()) {                      trace("down");                  }                  if (my_input.is_space()) {                      trace("space");                  }              }          }      }