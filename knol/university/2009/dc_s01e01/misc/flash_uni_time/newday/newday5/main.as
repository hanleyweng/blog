package {	import flash.display.Sprite;	import flash.events.Event;	public class main extends Sprite {		private var t=0;		private var fps=24;		private var midxstage=stage.stageWidth/2;		private var t_tran=4;//seconds		private var vel_tran_cur=0;		private var vel_tran_max=50;		private var int_tran=3;		private var pow_tran=2;//must always be even		private var ptx_tran_amp=100;		private var ptx_tran_a=midxstage-ptx_tran_amp;		private var ptx_tran_b=midxstage-ptx_tran_amp;		private var dir_tran=-1;//(ptx_tran_b-ptx_tran_a)/Math.abs(ptx_tran_b-ptx_tran_a);		private var wid_tran=Math.abs(ptx_tran_b-ptx_tran_a);		public var keyboard_input:keys;		public var char=new box  ;		public function main() {			addChild(char);			var keyboard_sprite = new Sprite();			addChild(keyboard_sprite);			keyboard_input=new keys(keyboard_sprite);			stage.addEventListener(Event.ENTER_FRAME,on_enter_frame);			//initial actions			char.y=225;			char.x=ptx_tran_a;		}		public function on_enter_frame(event:Event) {			if (keyboard_input.is_left()) {				//if (dir_tran==1) {				ptx_tran_a=char.x;				ptx_tran_b=midxstage-ptx_tran_amp;			}			if (keyboard_input.is_right()) {				//if (dir_tran==-1) {				ptx_tran_a=char.x;				ptx_tran_b=midxstage+ptx_tran_amp;				//}			}			//dir_tran = (ptx_tran_b-ptx_tran_a)/Math.abs(ptx_tran_b-ptx_tran_a);			wid_tran=Math.abs(ptx_tran_b-ptx_tran_a);			if (dir_tran*char.x<dir_tran*ptx_tran_b) {				vel_tran_cur = dir_tran*vel_tran_max*(1-Math.pow((2/wid_tran),pow_tran)*Math.pow((Math.abs(char.x-ptx_tran_a+dir_tran)-wid_tran/2),pow_tran));				//if (vel_tran_cur>0.01) {					char.x+=vel_tran_cur;					trace ("moving")				} else {					dir_tran=(midxstage-char.x)/Math.abs(midxstage-char.x);					trace ("stopped")				//}			}			if (dir_tran*ptx_tran_b<dir_tran*char.x) {				char.x=ptx_tran_b;			}			//code if char.x stays in midxstage for more than a few secs - teleport it		}	}}